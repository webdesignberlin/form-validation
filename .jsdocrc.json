{
  "tags": {
    "allowUnknownTags": true
  },
  "source": {
    "include": [
      "./src/",
      "./src/rules"
    ],
    "exclude": [ "./src/demo/" ],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "opts": {
    "destination": "./docs/"
  },
  "plugins": [],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false,
    "default": {
      "outputSourceFiles": true
    }
  }
}
